{
    "info": "Settings used for XWMT full test",
    "obslist": {},
    "settings": {
        "async": "none",
        "description": "Runs full example of the xwmt package",
        "driver": "full_example/full_example_driver.sh",
        "name": "full_example",
        "long_name": "XWMT Full Example Case"
    },
    "varlist": {
        "areaecello": {
          "standard_name" : "cell_area",
          "units": "m2",
          "dimensions": ["yh", "xh"]
        },
        "tos": {
          "standard_name" : "sea_surface_temperature",
          "units": "degC",
          "frequency" : "mon",
          "dimensions": ["time", "yh", "xh"]
        },
        "sos": {
          "standard_name" : "sea_surface_salinity",
          "units": "psu",
          "frequency" : "mon",
          "dimensions": ["time", "yh", "xh"]
        },
        "hfds": {
          "standard_name" : "surface_downward_heat_flux_in_sea_water",
          "units": "W m-2",
          "frequency" : "mon",
          "dimensions": ["time", "yh", "xh"]
        },
        "wfo": {
          "standard_name" : "water_flux_into_sea_water",
          "units": "kg m-2 s-1",
          "frequency" : "mon",
          "dimensions": ["time", "yh", "xh"]
        },
        "sfdsi": {
          "standard_name" : "downward_sea_ice_basal_salt_flux",
          "units": "kg m-2 s-1",
          "frequency" : "mon",
          "dimensions": ["time", "yh", "xh"]
        }       
    },
    "default_parameters": {
        "default1": 1
    },
    "data": 
        {
            "surface wmt data": 
                {
                    "filename": "model.G_2010to2014_sigma0_pacific_tropc.nc",
                    "long_name": "surfacce_wmt_data",
                    "description": "Surface watermass transformation data from model binned by lambda"
                }
        },
    "plots": 
        {
            "surface wmt comparison": 
		        {
                    "filename": "model.G_2010to2014_sigma0_pacific_tropc_comparison.png",
		    "long_name": "surface_wmt_comparison",
		    "description": "Surface watermass transformation from model compared with ECCO and ERA5"
                }
        },
    "provenance": 
        {
            "modeldata": "model_directory",
            "obsdata": "obs_directory",
            "log": "cmec-driver.XWMT.full_example.log.txt",
            "version": "xwmt==0.0.2",
            "environment": [
                "gsw==3.6.16",
                "numba==0.56.3",
                "numpy==1.23.4",
                "pandas==1.5.1",
                "xarray==2022.10.10",
                "xgcm==0.5.2",
                "xhistogram==0.3.1"
            ]
        }
}
